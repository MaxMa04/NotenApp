<?xml version="1.0" encoding="utf-8" ?>
<Application
    x:Class="NotenApp.App"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="PrimaryB">#3D3D3D</Color>
            <Color x:Key="SecondaryB">#ff82a9</Color>
            <Color x:Key="ThirdB">#ffc0be</Color>
            <Color x:Key="FourthB">#ffebe7</Color>
            <Color x:Key="FithB">#FFFFFF</Color>
            <Style TargetType="NavigationPage">
                <Setter Property="BarBackgroundColor" Value="{StaticResource SecondaryB}" />
                <Setter Property="BarTextColor" Value="{StaticResource FithB}" />
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>