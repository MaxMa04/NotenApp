<?xml version="1.0" encoding="utf-8" ?>
<Application
    x:Class="NotenApp.App"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="PrimaryB">#343434</Color>
            <Color x:Key="SecondaryB">#F0F4EF</Color>
            <Color x:Key="ThirdB">#344966</Color>
            <Color x:Key="FourthB">#B4CDED</Color>
            <Color x:Key="FithB">#BFCC94</Color>
            <Style TargetType="NavigationPage">
                <Setter Property="BarBackgroundColor" Value="{StaticResource ThirdB}" />
                <Setter Property="BarTextColor" Value="{StaticResource FithB}" />
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>